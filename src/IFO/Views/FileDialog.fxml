<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane>
         <children>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Name" />
                  <Label text="Path" GridPane.rowIndex="1" />
                  <Label text="Folder" GridPane.rowIndex="2" />
                  <Label text="Description" GridPane.rowIndex="3" />
                  <Label fx:id="nameInfo" text="Label" GridPane.columnIndex="1" />
                  <Label fx:id="pathInfo" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="folderInfo" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="descInfo" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Button fx:id="cancelButton" mnemonicParsing="false" text="Button" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <Label text="Tags" GridPane.rowIndex="4" />
                  <Label fx:id="tagsInfo" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
               </children>
            </GridPane>
         </children>
      </Pane>
   </children>
</AnchorPane>
